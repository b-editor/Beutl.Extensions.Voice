<UserControl x:Class="Beutl.Extensions.Voice.Views.TtsControllerView"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModels="using:Beutl.Extensions.Voice.ViewModels"
             d:DesignHeight="450"
             d:DesignWidth="800"
             x:CompileBindings="True"
             x:DataType="viewModels:TtsControllerViewModel"
             mc:Ignorable="d">
    <StackPanel Spacing="4">
        <ContentControl Name="Inner" DataContext="{Binding Inner}" />
        <Button Margin="8,0" Content="再生成" />
    </StackPanel>
</UserControl>
